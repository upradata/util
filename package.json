{
    "name": "@upradata/util",
    "author": {
        "name": "Thomas Milotti",
        "email": "thomas.milotti@gmail.com",
        "url": "https://github.com/milottit"
    },
    "repository": {
        "url": "milottit@vs-ssh.visualstudio.com:v3/milottit/Util%20Typescript/Util%20Typescript"
    },
    "license": "MIT",
    "version": "11.1.1",
    "description": "Set of typescript util tools",
    "sideEffects": false,
    "main": "lib",
    "types": "lib-esm",
    "module": "lib-esm",
    "files": [
        "lib",
        "lib-esm"
    ],
    "scripts": {
        "test": "npx jest --verbose false --config jest-ut.config.js",
        "e2e": "npx jest --passWithNoTests --verbose false --config jest-e2e.config.js",
        "clean": "shx rm -rf lib lib-esm",
        "prebuild": "npm run clean",
        "build": "tsc && tsc -m es6 --outDir lib-esm",
        "watch": "concurrently 'tsc -w' 'tsc -w -m es6 --outDir lib-esm'",
        "postbuild": "npm run test && npm run e2e",
        "build:only": "npm run prebuild && tsc && tsc -m es6 --outDir lib-esm",
        "build:watch": "npm run tsc -w",
        "github-push": "npm run build && npm version patch && git push && git push --tags",
        "npm-publish": "npm run build && npm publish --access public"
    },
    "devDependencies": {
        "@babel/runtime-corejs3": "^7.12.5",
        "@types/common-tags": "^1.8.0",
        "@types/jest": "^26.0.20",
        "@types/node": "^14.14.21",
        "concurrently": "^5.3.0",
        "core-js": "^3.8.2",
        "jest": "^26.6.3",
        "rxjs-compat": "^6.6.3",
        "shx": "^0.3.3",
        "ts-jest": "^26.4.4",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
        "tslint": "^6.1.3",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "common-tags": "^1.8.0",
        "emoji-regex": "^9.2.2",
        "find-up": "^5.0.0",
        "is-fullwidth-code-point": "^4.0.0",
        "rxjs": "^6.6.3",
        "ts-util-is": "^1.2.1"
    },
    "keywords": [
        "typescript",
        "util"
    ],
    "publishConfig": {
        "access": "public"
    },
    "local": {
        "usedBy": {
            "html-index-webpack-extended": "/home/milottit/Libraries/WebpackAngular/IndexHtmlPlugin"
        }
    }
}
